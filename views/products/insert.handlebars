
<div class="container conten">
    <h1>New Product</h1>
    {{#if model  }} 
        {{#each model}}
       <div class="alert alert-danger" role="alert">{{this}}</div> 
        {{/each}}
        {{/if}} 
    <form action="doInsertt" method="POST" ,enctype="multipart/form-data">
        <div class="form-group">
            <label for="name">Name</label>
            <input type="text" name="txtName" id="" class="form-control">
            <br>
        </div>
         <div class="form-group">
            <label for="description">Description</label>
            <input type="text" name="description" id="" class="form-control">
            <br>
        </div>
        <div class="form-group">
            <label for="image">Image</label>
            <input type="text " name="image" id="" class="form-control-file">
            <br>
        </div>
       
        <input type="submit" value="Create" class="btn btn-primary">
    </form>
</div>